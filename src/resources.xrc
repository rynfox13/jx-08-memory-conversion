                        </object>
                    </object>
                    <object class="sizeritem">
                        <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL|wxALL</flag>
                        <border>5</border>
                        <object class="wxHyperlinkCtrl" name="ID_HYPERLINKCTRL">
                            <style>wxHL_DEFAULT_STYLE</style>
                            <label>Built with wxWidgets</label>
                            <url>https://www.wxwidgets.org/</url>
                        </object>
                    </object>
                </object>
            </object>
            <object class="sizeritem">
                <flag>wxALIGN_CENTER_HORIZONTAL|wxALIGN_TOP|wxALL</flag>
                <border>5</border>
                <object class="wxButton" name="wxID_OK">
                    <label>OK</label>
                    <default>1</default>
                </object>
            </object>
        </object>
    </object>
